# print 10 new lines
echo -e "\n\n\n\n\n\n\n\n\n\n"
echo "__________starting python__________"
start_time=$(date +%s)
# Run the Python command and capture its exit status
python -m "$@"
PYTHON_EXIT_STATUS=$?
end_time=$(date +%s)

elapsed_time=$(( (end_time - start_time) / 60 ))
echo "Execution time: $elapsed_time minutes"